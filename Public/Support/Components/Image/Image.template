<img id="[ID]" [StyleClass:class="[]"] [Style:style="[]"] src="[path]" title="[TextValue]" />
<script type="text/javascript" charset="utf-8">
	SetComponent('[ID]', ComponentImage('[ID]'));
	_('[ID]').setState('main-image', '[path]');
	_('[ID]').setState('original-main-image', '[path]');
	_('[ID]').setState('alternative-image', '[alternativepath]');
	_('[ID]').setState('original-alternative-image', '[alternativepath]');
	_('[ID]').setState('other-image', '[otherpath]');
	_('[ID]').setState('original-other-image', '[otherpath]');
	[EventCode]
	_('[ID]').activate();
</script>
