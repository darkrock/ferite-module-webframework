namespace modifies GUIKit {

	class Popup extends GUIKit.FormSelect {
		function component
			return 'Popup';
			
		function componentRenderForMCAM {
			string code = super.componentRenderForMCAM();
			number selected = .selectedItem();
			if( selected >= 0 ) {
				code += "_('${.id}').selectItemsByValue($selected);";
			}
			return code;
		}
	}
}
